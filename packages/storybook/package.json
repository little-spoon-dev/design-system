{
  "name": "storybook",
  "private": true,
  "version": "2.1.0",
  "description": "Little Spoon Storybook",
  "scripts": {
    "build": "yarn copy-changelog && NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "clean": "rm -rf storybook-static src/stories/changelog",
    "copy-changelog": "mkdir -p src/stories/changelog && ts-node scripts/copy-changelog",
    "lint": "eslint . && yarn lint:tsc",
    "lint:fix": "eslint . --fix",
    "lint:tsc": "tsc --noEmit",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006"
  },
  "dependencies": {
    "@littlespoon/breadcrumbs": "1.3.4",
    "@littlespoon/button": "1.7.3",
    "@littlespoon/checkbox": "1.3.3",
    "@littlespoon/divider": "1.2.1",
    "@littlespoon/drawer": "1.1.0",
    "@littlespoon/icons": "2.0.1",
    "@littlespoon/theme": "1.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "babel-loader": "^8.2.2",
    "rgb2hex": "^0.2.5"
  }
}
