{
  "name": "storybook",
  "private": true,
  "version": "1.22.2",
  "description": "Little Spoon Storybook",
  "scripts": {
    "build": "yarn copy-changelog && build-storybook",
    "clean": "rm -rf storybook-static src/stories/changelog",
    "copy-changelog": "mkdir -p src/stories/changelog && ts-node scripts/copy-changelog",
    "lint": "eslint . && tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "@littlespoon/breadcrumbs": "1.2.3",
    "@littlespoon/button": "1.5.2",
    "@littlespoon/checkbox": "1.1.2",
    "@littlespoon/divider": "1.1.1",
    "@littlespoon/icons": "1.5.3",
    "@littlespoon/theme": "1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "babel-loader": "^8.2.2",
    "rgb2hex": "^0.2.5"
  }
}
